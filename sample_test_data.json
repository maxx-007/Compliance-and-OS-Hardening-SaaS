{
  "sample_config_upload": {
    "data": {
      "system_info": {
        "hostname": "web-server-01",
        "os": "Ubuntu 20.04 LTS",
        "kernel": "5.4.0-74-generic",
        "architecture": "x86_64"
      },
      "security_config": {
        "ssh": {
          "port": 22,
          "root_login": "no",
          "password_authentication": "yes",
          "permit_empty_passwords": "no",
          "max_auth_tries": 3
        },
        "firewall": {
          "status": "active",
          "default_incoming": "deny",
          "default_outgoing": "allow",
          "rules": [
            {"port": 22, "protocol": "tcp", "action": "allow", "source": "192.168.1.0/24"},
            {"port": 80, "protocol": "tcp", "action": "allow", "source": "any"},
            {"port": 443, "protocol": "tcp", "action": "allow", "source": "any"}
          ]
        },
        "users": [
          {"username": "admin", "uid": 1000, "groups": ["sudo", "admin"], "shell": "/bin/bash"},
          {"username": "webuser", "uid": 1001, "groups": ["www-data"], "shell": "/bin/bash"},
          {"username": "guest", "uid": 1002, "groups": ["users"], "shell": "/bin/sh"}
        ],
        "password_policy": {
          "min_length": 8,
          "require_uppercase": true,
          "require_lowercase": true,
          "require_numbers": true,
          "require_special_chars": false,
          "max_age_days": 90,
          "history_count": 5
        }
      },
      "services": {
        "running": ["ssh", "nginx", "mysql", "fail2ban"],
        "stopped": ["apache2", "ftp"],
        "enabled": ["ssh", "nginx", "mysql", "fail2ban"],
        "disabled": ["apache2", "ftp", "telnet"]
      },
      "network": {
        "interfaces": [
          {"name": "eth0", "ip": "192.168.1.100", "netmask": "255.255.255.0", "status": "up"},
          {"name": "lo", "ip": "127.0.0.1", "netmask": "255.0.0.0", "status": "up"}
        ],
        "open_ports": [22, 80, 443, 3306],
        "listening_services": [
          {"port": 22, "service": "ssh", "protocol": "tcp"},
          {"port": 80, "service": "nginx", "protocol": "tcp"},
          {"port": 443, "service": "nginx", "protocol": "tcp"},
          {"port": 3306, "service": "mysql", "protocol": "tcp"}
        ]
      }
    },
    "source": "system_scan",
    "type": "config",
    "metadata": {
      "scan_tool": "custom_scanner",
      "scan_version": "1.0",
      "scan_date": "2024-01-15T10:30:00Z",
      "target_system": "web-server-01"
    }
  },
